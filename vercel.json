{
    "buildCommand": "npm run build",
    "installCommand": "npm install",
    "framework": null,
    "redirects": [
        {
            "source": "/index-es.html",
            "destination": "/",
            "permanent": true
        },
        {
            "source": "/index-es",
            "destination": "/",
            "permanent": true
        }
    ],
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ]
}